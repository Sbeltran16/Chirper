<%- include('../partials/header') %> 
    
<h1 class="chirperHeader">Chirper</h1>
<% if(chirps.length) { %>
    <% let total = 0 %>
        <% chirps.forEach(function(c){ %>
            <div class="userContent"> 
            <%= c.chirpText %></br></br></br>
            <%= c.updatedAt %>
            <a href="/Chirper/chirp/<%= c._id %>">View Chirp</a>
            </div>
        <% }); %> 
<% } else { %>
    <h5 class="noPosts">No Chirps Yet</h5>
<% } %> 
</body>
</html>